<%- include ../views/partials/header.ejs %>

<%- include ../views/partials/navbar.ejs %>
<div class="container-fluid">
     <div class="row">
         <%- include ../views/partials/sidebar.ejs %>
         
         <main class="col-sm-9 offset-sm-3 col-md-10 offset-md-2 pt-3">
             <h1>Welcome! <img src="<%= user.avatar_url%>" alt="Yo" height="100px" width="100px"> </h1>
             <div class="list-group">
                 <% for(var i=0; i< repos.length; ++i) {%>
                    <button url="<%= repos[i].url %>" id="content_id" type="button" class="list-group-item list-group-item-action flex-column align-items-start">
                        <div class="d-flex w-150 justify-content-between" style="pointer-events:none;">
                            <h5 class="mb-1" style="pointer-events:none;"><%= repos[i].name %> </h5>
                        </div>
                        <small style="pointer-events:none;"><%= repos[i].description %></small>
                    </button>
                <% } %>
             </div>
         </main>
     </div>
</div>
<%- include ../views/partials/footer.ejs %>